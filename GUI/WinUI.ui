<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object class="GtkWindow" id="MainWin">
		<property name="default-height">500</property>
		<property name="default-width">1401</property>
		<property name="title">Oscilloscopio</property>
		<property name="resizable">FALSE</property>
		<child>
		  <object class="GtkBox">
		    <property name="orientation">0</property>
		    <child>
			    <object class="GtkDrawingArea" id="DisplayArea">
			      <property name="content-width">900</property>
			      <property name="content-height">500></property>
			    </object>
			  </child>
			  <child>
			    <object class="GtkGrid">
			      <property name="margin-start">10</property>
			      <property name="margin-top">10</property>
			      <property name="column-spacing">5</property>
			      <child>
			        <object class="GtkToggleButton" id="SE">
			          <property name="label">Start</property>
			          <property name="width-request">80</property>
			          <layout>
			            <property name="column">0</property>
			            <property name="row">0</property>
			          </layout>
			        </object>
			      </child>
			    </object>
			  </child>
			</object>
		</child>
	</object>
</interface>
